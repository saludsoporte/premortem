<div class="container form-control bg-white" data-controller="datos-personal">
  <% if params[:user].nil? %>
    <h3>Datos Personales de nuevo usuario</h3>
    <%= render partial: 'form',locals: {datos_personal: @datos_personal,nuevo:true} %>
  <% else %>
    <h3>Datos Personales</h3>
    <%= render partial: 'form',locals: {datos_personal: @datos_personal,nuevo:false} %>
  <% end %>
</div>